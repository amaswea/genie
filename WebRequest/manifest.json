{
    "name": "WebRequest",
    "description": "WebRequest - onCompleted",
    "version": "0.1",
    "permissions": ["<all_urls>", "webRequest"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ], 
            "run_at": "document_start"
        }
    ],
    "manifest_version": 2
}